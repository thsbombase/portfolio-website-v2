{
  "background": {
    "color": {
      "value": "#001939"
    },
    "repeat": "no-repeat",
    "size": "cover"
  },
  "fullScreen": {
    "zIndex": -1
  },
  "interactivity": {
    "events": {
      "onClick": {
        "enable": true,
        "mode": "push"
      },
      "onDiv": {
        "selectors": "#repulse-div",
        "mode": "repulse"
      },
      "onHover": {
        "enable": true,
        "mode": "connect",
        "parallax": {
          "force": 60
        }
      }
    },
    "modes": {
      "bubble": {
        "distance": 200,
        "duration": 2,
        "opacity": 0.8,
        "size": 40
      },
      "grab": {
        "distance": 400
      }
    }
  },
  "particles": {
    "color": {
      "value": "#ffffff"
    },
    "links": {
      "color": {
        "value": "#ffffff"
      },
      "distance": 100,
      "opacity": 0.3
    },
    "move": {
      "attract": {
        "rotate": {
          "x": 600,
          "y": 1200
        }
      },
      "enable": true,
      "outModes": {
        "bottom": "out",
        "left": "out",
        "right": "out",
        "top": "out"
      },
      "speed": 2
    },
    "number": {
      "density": {
        "enable": true
      },
      "limit": 100,
      "value": 70
    },
    "opacity": {
      "value": 0.4,
      "animation": {
        "speed": 1,
        "minimumValue": 0.1
      }
    },
    "size": {
      "random": {
        "enable": true
      },
      "value": {
        "min": 1,
        "max": 5
      },
      "animation": {
        "speed": 40,
        "minimumValue": 0.1
      }
    }
  }
}